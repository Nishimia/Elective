@model IEnumerable<Elective.Models.CourseModels.UserCourseViewModel>

@{
    ViewBag.Title = "PassedCourses";
}

<h2>PassedCourses</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mark)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mark)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { courseId = item.CourseId})
        </td>
    </tr>
}

</table>
